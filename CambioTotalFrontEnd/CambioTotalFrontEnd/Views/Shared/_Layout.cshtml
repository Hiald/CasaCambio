    <style>
        .loader-1 .loader-outter {
            position: absolute;
            border: 4px solid #f5c830;
            border-left-color: transparent;
            border-bottom: 0;
            width: 100%;
            height: 100%;
            border-radius: 50%;
            animation: loader-1-outter 1s cubic-bezier(0.42, 0.61, 0.58, 0.41) infinite;
        }

        .loader-1 .loader-inner {
            position: absolute;
            border: 4px solid #f5c830;
            border-radius: 50%;
            width: 40px;
            height: 40px;
            left: calc(50% - 20px);
            top: calc(50% - 20px);
            border-right: 0;
            border-top-color: transparent;
            animation: loader-1-inner 1s cubic-bezier(0.42, 0.61, 0.58, 0.41) infinite;
        }

        @@media (max-width: 1899px) {
            .banner .hero-area {
                padding: 100px 0 208px;
            }
        }

        .banner .mein-menu {
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            box-shadow: 0px 5px 89.61px 13.39px rgb(23 29 113 / 5%);
            background: -webkit-linear-gradient(top, #3dad52 100%, #3dad52 100%);
            z-index: 99999;
            padding: 10px 0 10px;
        }

        ::placeholder {
            /* Chrome, Firefox, Opera, Safari 10.1+ */
            color: black !important;
            opacity: 1;
            /* Firefox */
        }

        .banner {
            background-size: cover;
            background-position: center;
            background-repeat: no-repeat;
            padding-bottom: 120px;
            position: relative;
            z-index: 1;
        }

            .banner .hero-area .hero-form .courrency-select-Ip1 {
