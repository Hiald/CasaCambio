    <style>
        .loader-default:after {
            content: "";
            position: fixed;
            width: 48px;
            height: 48px;
            border: 0px solid #fff !important; 
            border-left-color: transparent;
            border-radius: 50%;
            top: calc(50% - 24px);
            left: calc(50% - 24px);
            animation: rotation 1s linear infinite;
        }

        .custom-control-label::before {
            position: absolute;
            top: .175rem;
            left: -1.5rem;
            display: block;
            width: 1rem;
            height: 1rem;
            pointer-events: none;
            content: "";
            background-color: #ffffff;
            border: #000000 solid 2px !important;
        }
        .side-nav .side-nav-link {
            color: #cedce4;
            display: block;
            padding: 9px 9px;
            font-size: .9375rem;
            position: relative;
            -webkit-transition: all .4s;
            transition: all .4s;
        }
        body[data-leftbar-theme=light] .help-box {
            background-color: #00a24f;
        }
    </style>

